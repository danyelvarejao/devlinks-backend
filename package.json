{
  "name": "devlinks-backend",
  "version": "1.0.0",
  "main": "dist/server.js",
  "license": "MIT",
  "author": "Danyel Varej√£o",
  "engines": {
    "node": "20"
  },
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "start": "node dist/server.js",
    "build": "tsup src",
    "postinstall": "npx prisma migrate deploy",
    "test:create-prisma-environment": "npm link ./prisma/vitest-environment-prisma",
    "test:install-prisma-environment": "npm link vitest-environment-prisma",
    "pretest:e2e": "run-s test:create-prisma-environment test:install-prisma-environment",
    "pretest:e2e:watch": "run-s test:create-prisma-environment test:install-prisma-environment",
    "test": "vitest run --dir src/usecases",
    "test:e2e": "vitest run --dir src/http",
    "test:e2e:watch": "vitest --dir src/http",
    "test:watch": "vitest --dir src/usecases",
    "test:coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@types/bcrypt": "5.0.2",
    "@types/node": "20.12.2",
    "@types/supertest": "6.0.2",
    "@typescript-eslint/eslint-plugin": "6.4.0",
    "@typescript-eslint/parser": "6.4.0",
    "@vitest/coverage-v8": "1.4.0",
    "eslint": "8.57.0",
    "eslint-config-love": "43.1.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-import-helpers": "1.3.1",
    "eslint-plugin-n": "16.6.2",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-promise": "6.1.1",
    "npm-run-all": "4.1.5",
    "prettier": "3.2.5",
    "prisma": "5.11.0",
    "supertest": "6.3.4",
    "tsup": "8.0.2",
    "tsx": "4.7.1",
    "typescript": "5.4.3",
    "vite-tsconfig-paths": "4.3.2",
    "vitest": "1.4.0"
  },
  "dependencies": {
    "@fastify/jwt": "8.0.0",
    "@prisma/client": "5.11.0",
    "bcrypt": "5.1.1",
    "dotenv": "16.4.5",
    "fastify": "4.26.2",
    "zod": "3.22.4"
  }
}
